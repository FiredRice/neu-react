{
	"$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
	"applicationId": "neutralino.react.template",
	"version": "1.0.0",
	"defaultMode": "window",
	"documentRoot": "/build/",
	"url": "/",
	"port": 0,
	"enableServer": true,
	"enableNativeAPI": true,
	"tokenSecurity": "one-time",
	"nativeAllowList": ["app.*", "filesystem.readDirectory"],
	"modes": {
		"window": {
			"title": "neu-react",
			"width": 1024,
			"height": 700,
			"minWidth": 640,
			"minHeight": 480,
			"center": true,
			"icon": "/build/logo192.png",
			"exitProcessOnClose": true,
			"enableInspector": false
		}
	},
	"logging": {
		"enabled": true,
		"writeToLogFile": false
	},
	"cli": {
		"binaryName": "neu-react",
		"resourcesPath": "/build/",
		"extensionsPath": "/extensions/",
		"binaryVersion": "4.12.0",
		"clientVersion": "3.1.0",
		"frontendLibrary": {
			"patchFile": "/public/index.html",
			"devUrl": "http://localhost:3000"
		}
	}
}
